name = "cubedos"
description = "A flight software framework for CubSat space missions."
version = "0.1.0-dev"

authors = ["Vermont State University"]
maintainers = ["Peter Chapin <peter.chapin@vermontstate.edu>"]
maintainers-logins = ["pchapin"]
[[depends-on]]
gnatprove = "^12.1.1"
